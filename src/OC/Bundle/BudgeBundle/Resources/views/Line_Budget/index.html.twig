{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Line_Budget list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Article</th>
                <th>Parag</th>
                <th>Sparag</th>
                <th>Region</th>
                <th>Labelline</th>
                <th>Ministere</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('line_budget_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.article }}</td>
                <td>{{ entity.parag }}</td>
                <td>{{ entity.sParag }}</td>
                <td>{{ entity.region }}</td>
                <td>{{ entity.labelLine }}</td>
                <td>{{ entity.ministere }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('line_budget_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('line_budget_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('line_budget_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
